programa {
  funcao inicio() {
    
    inteiro base
    inteiro altura


    escreva("Qual é o valor da base do Retângulo que você gostaria de calcular a área? ")
    leia(base)
    escreva("Qual é o valor da altura? ")
    leia(altura)

    inteiro triangulo = base * altura / 2

    escreva("A área do Losango é de: ", triangulo)
  }
}
