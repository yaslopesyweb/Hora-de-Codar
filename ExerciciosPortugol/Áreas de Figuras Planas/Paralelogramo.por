programa {
  funcao inicio() {
    
    inteiro base
    inteiro altura

    escreva("Qual é o valor da base do Paralelogramo que você gostaria de calcular a área? ")
    leia(base)
    escreva("Qual é o valor da altura? ")
    leia(altura)

    inteiro paralelogramo = base * altura
    escreva("A área do Paralelogramo é de ", paralelogramo)

  }
}
